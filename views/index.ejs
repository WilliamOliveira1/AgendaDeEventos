<%- include('./partials/head') %>
<body>
    <div class="container">
      <div class="row">
        <h2>Sistema de Agendamento de eventos</h2>
        <div class="col">
          <form class="">
            <div class="form-group m-2">
              <label class="m-2" for="InputEmail">Usuário</label>
              <input type="email" class="form-control" id="InputUser" placeholder="Digite o usuário">
            </div>
            <div class="form-group m-2">
              <label for="InputPassword">Password</label>
              <input type="password" class="form-control" id="InputPassword" placeholder="Senha">
            </div>
            <div class="form-check m-2">
              <input type="checkbox" class="form-check-input" id="Check">
              <label class="form-check-label" for="Check">Lembrar</label>
            </div>
            <button type="submit" class="btn btn-primary m-2">Entrar</button>
          </form>
        </div>        
      </div>      
    </div> 
</body>
<%- include('partials/footer'); %>